<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Admin.master.cs" Inherits="stadium_management.ClientUi.Admin" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" />
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="/ClientUi/Styles/bootstrap.min.css" />
    <!-- Material Design Bootstrap -->
    <link rel="stylesheet" href="/ClientUi/Styles/mdb.min.css" />

    <script src="/ClientUi/Scripts/bootstrap.min.js"></script>

    <link href="Styles/Styles.css" rel="stylesheet" />

    <style type="text/css">
        html, body {
            padding-top: 50px;
            margin: 0;
            padding: 0;
            width: 100%;
            height: 100%;
            overflow: auto;
        }
    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="/ClientUi/Scripts/mdb.min.js"></script>

    <form runat="server" style="min-height: 93%">
        <header>
            <nav class="navbar-fixed-top navbar navbar-dark bg-primary navbar-expand-lg fixed-top">

                <asp:Label CssClass="navbar-brand" ID="lblName" runat="server"></asp:Label>

                <div class="container ">
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNavDropdown">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/ClientUi/Pages/HomePublic.aspx">Inicio</a>
                            </li>
                        </ul>
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/UIClient/Pages/AdministratorManagement.aspx">Administradores</a>
                            </li>
                        </ul>
                    </div>

                    <ul class="navbar-nav ml-auto nav-flex-icons">
                        <li runat="server" class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown"
                                aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-user">  Mi Cuenta</i>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right dropdown-default"
                                aria-labelledby="navbarDropdownMenuLink">
                                <asp:Button class="dropdown-item" ID="btnEndSession" Text="Cerrar sesion" runat="server" CausesValidation="false" OnClick="btnEndSession_Click" />
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <asp:ContentPlaceHolder ID="MainContentAdmin" runat="server" />

    </form>
    <footer class="page-footer font-small bg-primary">
        <div class="container">
            <div class="text-center py-3">
                ©️ 2021 Copyright: Pedro Urrutia
            </div>
        </div>
    </footer>
</body>
</html>
